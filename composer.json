{
    "name": "maghead/maghead",
    "homepage": "https://github.com/maghead/maghead",
    "keywords": [
        "database",
        "generator",
        "orm",
        "model",
        "collection",
        "sql",
        "fast"
    ],
    "description": "The Fast PHP ORM",
    "require": {
        "php": ">=7.0",
        "corneltek/configkit": "^1.6.2",
        "corneltek/cascading-attribute": "^1.2",
        "corneltek/cliframework": "4.0.x-dev",
        "corneltek/codegen": "4.0.x-dev",
        "corneltek/serializerkit": "^1",
        "corneltek/validationkit": "^1",
        "maghead/sqlite-parser": "^4.0",
        "maghead/magsql": "4.0.x-dev",
        "symfony/yaml": "^2.8|^3.0|^3.2",
        "doctrine/inflector": "~1.0",
        "pimple/pimple": "^3.0",
        "cypresslab/gitelephant": "^1.0",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.5",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "mongodb/mongodb": "^1.1",
        "ext-apcu": "*"
    },
    "suggest": {
        "mongodb/mongodb": "^1.1",
        "ext-xarray": "*"
    },
    "bin": [
        "bin/maghead"
    ],
    "support": {
        "issues": "https://github.com/maghead/maghead/issues"
    },
    "license": "MIT",
    "authors": [
        { "name": "Yo-An Lin", "email": "cornelius.howl@gmail.com" }
    ],
    "extra": { "branch-alias": { "dev-master": "4.0.x-dev" } },
    "autoload": {
        "psr-4": { "Maghead\\": "src/" }
    },
    "autoload-dev": {
        "classmap": ["tests"],
        "psr-4": {
            "AuthorBooks\\": "examples/books/",
            "MetricApp\\": "examples/metric/",
            "PageApp\\": "examples/page/",
            "StoreApp\\": "tests/apps/StoreApp/",
            "TestApp\\": "tests/apps/TestApp/",
            "Todos\\": "examples/todos/src/Todos/"
        }
    }
}
